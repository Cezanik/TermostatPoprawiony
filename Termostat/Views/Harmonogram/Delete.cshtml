@model Harmonogram

<h2>Czy na pewno chcesz usunąć ten harmonogram?</h2>

<div class="card">
    <div class="card-body">
        <h5 class="card-title">@Model.Nazwa</h5>
        <p class="card-text">
            <strong>Start:</strong> @Model.Start:00<br>
            <strong>End:</strong> @Model.End:00<br>
            <strong>Temperatura:</strong> @Model.DocelowaTemperatura °C
        </p>
    </div>
</div>

<form asp-action="Delete" method="post" class="mt-3">
    <input type="hidden" asp-for="Id" />
    <button type="submit" class="btn btn-danger">Usuń</button>
    <a asp-action="Index" class="btn btn-secondary">Anuluj</a>
</form>